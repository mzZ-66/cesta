<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>test</servlet-name>
        <servlet-class>Controlador.test</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>sv_obtenerAlimentos</servlet-name>
        <servlet-class>Controlador.sv_obtenerAlimentos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>sv_modificarLimiteGasto</servlet-name>
        <servlet-class>Controlador.sv_modificarLimiteGasto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>sv_anadirAlCarrito</servlet-name>
        <servlet-class>Controlador.sv_anadirAlCarrito</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>test</servlet-name>
        <url-pattern>/test</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>sv_obtenerAlimentos</servlet-name>
        <url-pattern>/sv_obtenerAlimentos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>sv_modificarLimiteGasto</servlet-name>
        <url-pattern>/sv_modificarLimiteGasto</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>sv_anadirAlCarrito</servlet-name>
        <url-pattern>/sv_anadirAlCarrito</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
